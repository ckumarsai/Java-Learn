package Inheritance;

class  Vahicle {
	public int wheels;
	public String colour;	
}
class Car extends Vahicle {
	void CarWheels(int wheels) {
		System.out.println("car has"+wheels+"wheels");
	}
	
}
public class HeirarchicalInheritance {
	public static void main(String[] args) {
		Car c=new Car();
		c.CarWheels(4);
	}
	

}
